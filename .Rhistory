geom_endpoint <- function(
mapping = NULL,
data = NULL,
stat = "identity",
position = "identity",
...,
na.rm = FALSE,
show.legend = NA,
inherit.aes = TRUE
) {
endpoints <- bind_rows(
filter(data, x == max(x)),
filter(data, x == min(x))
)
geom_point(data = endpoints, size = 8) +
geom_point(data = endpoints, size = 3, color = "white")
}
